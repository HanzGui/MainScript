--- Gravity ---

NoGravity.MouseButton1Click:Connect(function()
	game.Workspace.Gravity = 0
end)

NormalGravity.MouseButton1Click:Connect(function()
	game.Workspace.Gravity = 196.2
end)

LowGravity.MouseButton1Click:Connect(function()
	game.Workspace.Gravity = 100
end)




--- WalkSpeed Local player ---




SpeedNormal.MouseButton1Click:Connect(function()
	game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
end)


Speed40.MouseButton1Click:Connect(function()
	game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 40
end)


Speed1225.MouseButton1Click:Connect(function()
	game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 1225
end)


---Other Fun scripts ---
Step.MouseButton1Click:Connect(function()
	while true do
		print("")
	end
end)





Person.MouseButton1Click:Connect(function()
	while true do
		print("")
	end
end)





Funny.MouseButton1Click:Connect(function()
	while true do
		print("")
	end
end)





OpenCredits.MouseButton1Click:Connect(function()
	Credits.Visible = true
end)







OpenInfo.MouseButton1Click:Connect(function()
	Info.Visible = true
end)







SubTheDub.MouseButton1Click:Connect(function()
	game.Workspace.Gravity = 0
	game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 100000000
end)

Flidar3.MouseButton1Click:Connect(function()
	PinkFlidar3.Visible = true
	PinkFlidar3.ClipsDescendants = true
end)







Flidar3.MouseButton2Click:Connect(function()
	PinkFlidar3.Visible = false
end)







OpenLocalPlayer.MouseButton1Click:Connect(function()
	LocalPlayer.Visible = true
end)


OpenLocalPlayer.MouseButton2Click:Connect(function()
LocalPlayer.Visible = false
end)




Close2.MouseButton1Click:Connect(function()
	Credits.Visible = false
end)



TextButton.MouseButton1Click:Connect(function()
	MainFrame.Visible = true
	EnterFrame.Visible = false
end)



Close.MouseButton1Click:Connect(function()

end)


--- Draggable scripts ---

LocalPlayer.Active = true
LocalPlayer.Draggable = true

MainFrame.Active = true
MainFrame.Draggable = true

EnterFrame.Active = true
EnterFrame.Draggable = true
