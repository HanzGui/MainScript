--- Button codes ---

EnterButton.MouseButton1Click:Connect(function()
	MainGUI.Visible = true
	EnterGUI.Visible = false
end)

OpenInfo.MouseButton1Click:Connect(function()
	Info.Visible = true
end)

Exit2.MouseButton1Click:Connect(function()
	Info.Visible = false
end)

OpenCredits.MouseButton1Click:Connect(function()
	Credits.Visible = true
end)

Exit.MouseButton1Click:Connect(function()
	Credits.Visible = false
end)

OpenLocalPlayer.MouseButton1Click:Connect(function()
	LocalPlayer.Visible = true
end)

OpenLocalPlayer.MouseButton2Click:Connect(function()
	LocalPlayer.Visible = false
end)

CoolButton.MouseButton1Click:Connect(function()
	print("You are cool")
end)

CoolerButton.MouseButton1Click:Connect(function()
	print("You are very cool")
end)

Funny.MouseButton1Click:Connect(function()
	print("Don't press the funnier button!")
end)

Funnier.MouseButton1Click:Connect(function()
	while true do
		print("Lol dis funny")
	end
end)

SubTheDubMode.MouseButton1Click:Connect(function()
	game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 100000
	game.Workspace.Gravity = 0
end)

NO.MouseButton1Click:Connect(function()
	while true do
		print("NO")
		wait(0)
	end
end)

Nice.MouseButton1Click:Connect(function()

	local Pos = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
	local Rock = Instance.new("Part")
	Rock.Parent = game.Workspace
	Rock.Position = Pos

end)

EvenNicer.MouseButton1Click:Connect(function()
	while true do
		local Pos = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
		local Rock = Instance.new("Part")
		Rock.Parent = game.Workspace
		Rock.Position = Pos
		wait(0.1)
	end
end)

FlidarButton.MouseButton1Click:Connect(function()
	flidar3Mode.Visible = true
end)

FlidarButton.MouseButton2Click:Connect(function()
	flidar3Mode.Visible = false
end)

HWID.TextScaled = true
HWID.Text = game:GetService("RbxAnalyticsService"):GetClientId()

Speed1225.MouseButton1Click:Connect(function()
	game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 1225
end)

Speed2450.MouseButton1Click:Connect(function()
	game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 2450
end)

WS40.MouseButton1Click:Connect(function()
	game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 40
end)

WS16.MouseButton1Click:Connect(function()
	game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
end)

NoGravity.MouseButton1Click:Connect(function()
	game.workspace.Gravity = 0
end)

SuperLowGravity.MouseButton1Click:Connect(function()
	game.Workspace.Gravity = 50
end)

LowGravity.MouseButton1Click:Connect(function()
	game.Workspace.Gravity = 100
end)

NormalGravity.MouseButton1Click:Connect(function()
	game.Workspace.Gravity = 196.2
end)

MainGUI.Active = true
MainGUI.Draggable = true

LocalPlayer.Active = true
LocalPlayer.Draggable = true
